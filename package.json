{
  "homepage": "https://github.com/axa-ch/pod-adaptive-cards-testings#readme",
  "name": "@axa-ch/pod-adaptive-cards-testings",
  "version": "0.0.4",
  "main": "lib/index.js",
  "description": "This project was used for a hackaton about adaptive cards.",
  "scripts": {
    "build": "npm run clean-lib && cross-env NODE_ENV=production rollup --config ./rollup.config.js",
    "build-dev": "cross-env NODE_ENV=development CONFIG=local rollup --config ./rollup.config.js --watch",
    "clean-lib": "rimraf lib",
    "copy-podproperties-to-indexhtml": "node -e \"require('./__mock/copypropstohtml.js').copyPropsToHtml()\"",
    "mockServer": "rimraf ./src/mocks/.tmp && node ./src/mocks/init-mock.js && ts-node -r tsconfig-paths/register -r ./tsnode-setup.js --files ./src/mocks/server.ts port=8081 --host 0.0.0.0",
    "icons:type-gen": "node ./utils/icon-type-gen.js",
    "release": "npm whoami && npm install && npm run build && npm publish",
    "prestart": "npm run icons:type-gen",
    "start": "npm run copy-podproperties-to-indexhtml && concurrently \"webpack-dev-server\" \"npm run mockServer\"",
    "test": "eslint src/ && npm run test-jest",
    "test-jest": "node scripts/test.unit.js --env=jsdom",
    "test-jest-watch": "node scripts/test.unit.js --env=jsdom --watch",
    "translations": "ts-node utils/translations/index.ts",
    "type-check": "tsc"
  },
  "dependencies": {
