{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "array-type": [
      true,
      "array-simple"
    ],
    "import-name": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "interface-over-type-literal": false,
    "max-line-length": {
      "options": [
        140
      ]
    },
    "no-boolean-literal-compare": true,
    "object-shorthand-properties-first": false,
    "prefer-array-literal": false,
    "prefer-template": [
      true,
      "allow-single-concat"
    ],
    "strictNullChecks": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ]
  },
  "rulesDirectory": []
}
